<% flash.each do |type, message| %>
  <div class="alert <%= "alert-#{type}" %>"><%= message %></div>
<% end %>
<h1 class="hero_title">List of teachers</h1>
<div class="btn_container">
  <%= link_to "New student", new_user_path, class: "btn_user" %>
</div>
<div class="user_table">
  <p>Name</p>
  <p>Email</p>
  <p>Options</p>
</div>
<div class="user_container">
  <% @users.each do |user| %>
    <%= render user %>
  <% end %>
</div>

